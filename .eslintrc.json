{
  "extends": "@rocketseat/eslint-config/react",
  "ignorePatterns": ["node_modules"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
      "printWidth": 80,
      "tabWidth": 2,
      "singleQuote": true,
      "trailingComma": "all",
      "arrowParens": "always",
      "semi": true,
      "endOfLine": "auto"
    }]
  }
}